<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xing.android.calendarpicker.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CalendarHomeDemoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SingleWeekActivity" />
        <activity android:name=".WeekListActivity" />
        <activity android:name=".MonthListActivity" />
        <activity android:name=".YearListActivity" />
        <activity android:name=".MultiViewActivity" />
        <activity android:name=".VPageWeekCalendarActivity" />
        <activity android:name=".HPageMonthCalendarActivity" />
    </application>

</manifest>